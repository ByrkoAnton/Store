@model Store.BusinessLogicLayer.Models.UserSignInModel
@{
Layout = "~/_Layout.cshtml";
}

<h2>Sign In</h2>
<form method="post" asp-controller="AdminAccount" asp-action="SignIn">

    <div>
        <label asp-for="Email"></label><br />
        <input asp-for="Email" />
    </div>
    <div>
        <label asp-for="Password"></label><br />
        <input asp-for="Password" />
    </div>
    <div>
        <input type="submit" value="Sign In" />
    </div>
</form>

